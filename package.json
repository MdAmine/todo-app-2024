  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
